server {
        listen 80 default_server;
        listen [::]:80 default_server;

        location / {
                root /var/www/html/;
                try_files $uri $uri/ =404;
        }


        location /airbnb-onepage/ {
                proxy_pass http://localhost:5000;
        }

        location = /404.html {
                root /var/www/error/;
                internal;
        }
}
